{
  "name": "v-monitor",
  "version": "0.0.1",
  "description": "Frontend monitoring system monorepo",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @monitor/server dev",
    "dev:server": "pnpm --filter @monitor/server start:dev",
    "dev:playground": "pnpm --filter playground start:dev",
    "dev:web": "pnpm --filter @monitor/web dev",
    "build": "pnpm -r build",
    "build:monitor": "pnpm --filter @monitor/sdk build",
    "build:playground": "pnpm --filter playground build",
    "build:server": "pnpm --filter @monitor/server build",
    "build:web": "pnpm --filter @monitor/web build",
    "build:web:with-maps": "pnpm --filter @monitor/web run build:with-maps",
    "build:web:with-maps:sudo": "sudo -E env pnpm --filter @monitor/web run build:with-maps",
    "clean": "pnpm -r clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "db:generate": "pnpm --filter server db:generate",
    "db:migrate": "pnpm --filter server db:migrate",
    "db:migrate:deploy": "pnpm --filter server db:deploy",
    "db:studio": "pnpm --filter server prisma studio",
    "server:up": "pnpm --filter @monitor/server docker-compose up -d",
    "server:down": "pnpm --filter @monitor/server docker-compose down",
    "server:logs": "pnpm --filter @monitor/server docker-compose logs -f"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/pako": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "pako": "^2.1.0"
  },
  "keywords": [
    "monitoring",
    "frontend",
    "error-tracking",
    "performance"
  ],
  "author": "",
  "license": "MIT"
}
